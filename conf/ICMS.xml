<?xml version="1.0" encoding="UTF-8"?>
<ICMSConfig>

  <ftpConfigs>
    <merlin>
      <!-- Configuration for Merlin server, where we get Bar feeds -->
      <host>merlin.flcourts.org</host>
      <user>XXXXXX</user>
      <password>XXXXXX</password>
    </merlin>
  </ftpConfigs>

  <ldapConfig>
    <ldapHost>[LDAP/AD URL]</ldapHost>
    <bindDn>[DN OF USER FOR BINDING TO AD]</bindDn>
    <bindPw>[PASSWORD FOR USER ABOVE]</bindPw>
    <userBase>[DN FOR THE AD USER BASE]</userBase>
    <svcBase>[DN FOR THE AD SERVICE BASE\</svcBase>
    <!-- filterFormat (below) is a filter applied to the sAMAccountName that must match your user. This example is for the 15th Circuit's configuration,
	 but it's important to have the %s substitution - it's populated with a sprintf() later -->
    <filterFormat>(&amp;(sAMAccountName=%s)(memberof:1.2.840.113556.1.4.1941:=CN=CAD-ICMS-GROUP,OU=Services,OU=CAD,OU=Enterprise,DC=pbcgov,DC=org))</filterFormat>
  </ldapConfig>

  <vrbUrl>[YOUR VIRTUAL REDBOOK URL]</vrbUrl>

  <olsURL>[URL to your Online Scheduling Application]</olsURL>

  <showCaseDb>showcase-prod</showCaseDb>
  <signKey>16charactersdede</signKey>
  <baseURL>https://jvs-dev.15thcircuit.com/case/</baseURL>
  <baseURLSearch>https://jvs-dev.15thcircuit.com/cgi-bin/case/search.cgi</baseURLSearch>

  <clerkEmail>
    <name>Circuit Civil</name>
    <CourtTypes>CA</CourtTypes>
    <EmailAddr>CircuitCourtOrders@mypalmbeachclerk.com</EmailAddr>
  </clerkEmail>
  <clerkEmail>
    <name>County Civil</name>
    <CourtTypes>CC,SC</CourtTypes>
    <EmailAddr>CircuitCourtOrders@mypalmbeachclerk.com</EmailAddr>
  </clerkEmail>

  <!-- For communicating with the state e-filing portal -->
  <serviceListWsdl>https://test.myflcourtaccess.com/wsdl/ElectronicServiceListService.wsdl</serviceListWsdl>
  <eFilingWsdl>https://test.myflcourtaccess.com/wsdl/BulkFilingReviewService.wsdl</eFilingWsdl>

  <portalAdminFiler>
    <name>[PORTAL ADMIN USER]</name>
    <password>[USER PASSWORD]</password>
  </portalAdminFiler>

  <icmsWebService>https://icms-web.15thcircuit.com</icmsWebService>

  <dbConfig>
    <name>showcase-prod</name>
    <dbType>mssql</dbType>
    <!-- Need to specify "server=" with SQL-Server; the hostname should correspond
                     to an entry in /etc/freetds.conf -->
    <dbHost>XXXXXX</dbHost>
    <dbUser>icmsapp</dbUser>
    <dbPass>XXXXXX</dbPass>
    <dbName>ShowCase</dbName>
  </dbConfig>

  <dbConfig>
    <name>showcase-rpt</name>
    <dbType>mssql</dbType>
    <!-- Need to specify "server=" with SQL-Server; the hostname should corresponf
		to an entry in /etc/freetds.conf -->
    <dbHost>server=showcase-rpt</dbHost>
    <dbUser>icmsapp</dbUser>
    <dbPass>XXXXXX</dbPass>
    <dbName>ShowCase</dbName>
  </dbConfig>

  <dbConfig>
    <name>icms</name>
    <dbType>mysql</dbType>
    <dbHost>DBHOST</dbHost>
    <dbUser>DBUSERNAME</dbUser>
    <dbPass>DBPASSWORD</dbPass>
