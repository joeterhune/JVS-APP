                                                        <td class="center">[% IF charge.Statute != 'VOP' %]&#167; [% END %][% charge.Statute %][% IF charge.SubSection %] ([% charge.SubSection %])[% END %]</td>
                                                        <td class="center">[% charge.CitationNumber %]</td>
                                                        <td class="center">[% charge.ChargeDescription %]</td>
                                                        <td class="center">[% charge.ChargeLevel %][% charge.ChargeDegree %]</td>
                                                        <td class="center">[% charge.DispCode %]</td>
                                                        <td class="center">[% charge.DispDate %]</td>
                                                    </tr>
                                                    [% END %]
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                    [% END %]
                                </table>
                            </div>
                            [% END %]
                
                            <!-- Events -->
                            <div class="spacer"></div>
                            <div id="eventdiv_[% data.ucn %]">
                                <table class="summary">
                                    <tr>
                                        <td class="title">Case Calendar Event Docket</td>
                                    </tr>
                                    [% IF data.events.size == 0 %]
                                    <tr>
                                        <td>There are no events for this case.</td>
                                    </tr>
                                    [% ELSE %]
                                    <tr>
                                        <td>
                                            <table id="events_[% data.ucn %]">
                                                <thead>
                                                    <tr class="title">
                                                        <th style="min-width: 6em">Date</th>
                                                        <th style="min-width: 6em">Code</th>
                                                        <th style="min-width: 100px">Description</th>
                                                        <th style="min-width: 100px">Location/<br/>Room</th>
                                                        [% IF data.hasMotions %]<th style="min-width: 15em">Motion</th>[% END %]
                                                        <th style="min-width: 100px">Time</th>
                                                        <th style="min-width: 75px">Judge</th>
                                                        <th style="min-width: 100px">Docket Date</th>
                                                        <th style="min-width: 100px">Canceled?</th>
                                                        <th style="min-width: 100px">Updated?</th>
                                                        <th style="min-width: 100px">Reason</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    [% FOREACH event IN data.events %]
                                                    <tr class="[% event.RowClass %]">
                                                        <td class="[% event.RowClass %]" style="text-align: center">
                                                            [% IF event.ShowVRBLink == "future" %]
                                                            <a class="vrbDate" data-vrbdate="[% event.ISODate %]" data-event_id="[% event.EventID %]">[% event.EventDate %]</a>
                                                            [% ELSE %]
                                                            [% event.EventDate %]
                                                            [% END %]
                                                        </td>
                                                        <td class="center [% event.RowClass %]">[% event.EventCode %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.CourtEventType %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.Location %]</td>
                                                        [% IF data.hasMotions %]<td class="center [% event.RowClass %]">[% event.Motion %]</td>[% END %]
                                                        <td class="center [% event.RowClass %]">[% event.CourtEventTime %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.JudgeName %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.DocketDate %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.Canceled %]</td>
                                                        <td class="center [% event.RowClass %]">[% event.Updated %]</td>
                                                        [% IF event.Updated == 'Y' %]
                                                        	<td class="[% event.RowClass %]" style="text-align: center">[% event.update_info %]</td>
                                                        [% ELSIF event.Canceled == 'Y' %]
                                                        	<td class="[% event.RowClass %]" style="text-align: center">[% event.cancel_reason %]</td>
                                                        [% ELSE %]
                                                          	<td class="[% event.RowClass %]" style="text-align: center">&nbsp;</td>
                                                        [% END %]
                                                    </tr>
                                                    [% END %]
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                    [% END %]
                                    <tr>
                                        <td>
                                            <button type="button" class="btn btn-primary vrbCase printHide" data-ucn="[% data.caseinfo.CaseNumber %]">Schedule Event</button>
                                        </td>
                                    </tr>
                                
                                </table>
                            </div>
                            
                            <!--<div>
                                <pre>
                                    [% USE Dumper %]
                                    [% Dumper.dump (data.otherDocs) %]
                                </pre>
                            </div>-->
                            
                            <div class="spacer"></div>
                            <div id="otherdocsdiv_[% data.ucn %]">
                                <table class="summary">
                                    <tr>
                                        <td class="title">Other Documents</td>
                                    </tr>
                                    [% IF data.otherDocs.size == 0 %]
                                    <tr>
                                        <td>There are no other documents for this case.</td>
                                    </tr>
                                    [% ELSE %]
                                    <tr>
                                        <td>
                                            <table id="otherdocs_[% data.ucn %]">
                                                <thead>
                                                    <tr class="title">
                                                        <th style="min-width: 20em">Title</th>
                                                        <th style="min-width: 6em">Date Created</th>
                                                        <th style="min-width: 6em">Date Due</th>
                                                        <th style="min-width: 20em">Comments</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    [% FOREACH doc IN data.otherDocs %]
                                                    <tr>
                                                        <td class="center"><a class="[% doc.doc_type %]" data-docid="[% doc.DocID %]" data-casenum="[% data.caseinfo.CaseNumber %]">[% doc.Title %]</a></td>
                                                        <td class="center">[% doc.CreationDate %]</td>
                                                        <td class="center">[% doc.DueDate %]</td>
                                                        <td class="center">
                                                            [% doc.Comments %]
                                                            <button class="wfmenubut2" class="btn btn-primary" style="display: none" data-doc_id="[% doc.DocID %]" data-ucn="[% data.caseinfo.CaseNumber %]"></button>
                                                            <a class="orders printhide" data-doc_id="[% doc.DocID %]" data-ucn="50[% data.ucn %]" data-parent-tab="case-[% data.caseinfo.CaseNumber %]" style="display: none">&nbsp;</a>
                                                        </td>
                                                    </tr>
                                                    [% END %]
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                    [% END %]
                                </table>
                            </div>
                
                            <!-- Dockets -->
                            <div class="spacer"></div>
                            <div id="docketdiv_[% data.ucn %]">
                                Retrieving Dockets...
                            </div>
                
                            <!-- Other cases -->
                            <div class="spacer"></div>
                            <div id="othercases_[% data.ucn %]">
                            [% FOREACH party IN data.parties %]
                                [% IF (party.OtherCases.size) && (party.OtherCases.size > 0) %]
                                <table class="summary">
                                    <tr>
                                        <td colspan="5" class="title">
                                            Other Cases for [% party.FullName %] ([% party.PartyTypeDesc %])
                                        </td>
                                    </tr>
                
                                    <tr>
                                        <td>
                                            <table class="otherCases">
                                                <thead>
                                                    <tr class="title">
                                                        <th style="width: 10em">CaseNumber</th>
                                                        <th style="width: 7em">Party Type</th>
                                                        <th style="width: 5em">Case<br/>Type</th>
                                                        <th style="width: 30em">Case Style</th>
                                                        <th style="width: 5em">Status</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    [% FOREACH otherCase IN party.OtherCases %]
                                                    <tr>
                                                        <td style="text-align: center">
                                                            <a class="caseLink" data-casenum="[% otherCase.CaseNumber %]">
                                                                [% otherCase.CaseNumber %]
                                                            </a>
                                                        </td>
                                                        <td style="text-align: center">[% otherCase.PartyTypeDesc %]</td>
                                                        <td style="text-align: center">[% otherCase.CaseType %]</td>
                                                        <td style="text-align: left">[% otherCase.CaseStyle %]</td>
                                                        <td style="text-align: center">[% otherCase.Status %]</td>
                                                    </tr>
                                                    [% END %]
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                                [% END %]
                            [% END %]
                            </div>

                            <div id="hiddenvars_[% data.ucn %]">
                                <input type="hidden" id="division_[% data.ucn %]" name="division" value="[% data.caseinfo.DivisionID %]"/>
                                <input type="hidden" id="nextlev_[% data.ucn %]" name="nextlev" value="[% data.nextlev %]"/>
                                <input type="hidden" id="ucn_[% data.ucn %]" name="ucn" value="[% data.caseinfo.CaseNumber %]"/>
                                <input type="hidden" class="ucn"value="[% data.caseinfo.CaseNumber %]"/>
                                <input class="caseType" type="hidden" value="[% data.caseinfo.CaseType %]"/>
                            </div>
                        
                        </div>
                        
                    </div> <!-- End of drawer-content -->
                </div> <!-- End of drawer-full -->
                
                <div class="content closed" style="display: none; height: 100%">
                    <div id="documents_[% data.ucn %]" class="dv-window" style="height: 90%; width: 100%;">
                        
                    </div>
                </div>
            </div> <!-- End of drawer-container -->
            
            