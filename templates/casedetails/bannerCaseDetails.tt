            <script type="text/javascript">
            $(document).ready(function() {
                bannerGetAsync('[% data.ucn %]', '[% data.caseinfo.CaseNumber %]', '[% data.caseinfo.CaseType %]');

                $("table thead th:eq(6)").data("sorter", false);

                $('#warrants_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#olsevents_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#charges_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#events_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#related_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#reopens_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#otherdocs_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('.otherCases').tablesorter({widgets: ['zebra']});
                
                $('table').on("click", ".checkallboxes", function () {
					$(this).closest('table').find(':checkbox').prop('checked', true);
				});
				$('table').on("click", ".uncheckallboxes", function () {
					$(this).closest('table').find(':checkbox').prop('checked', false);
				});
				
                $('#docketdiv_[% data.ucn %]').on("click", ".toggleDocket", function () {
					$('#docketTable_[% data.ucn %]').toggle();
					var position = $('#docketdiv_[% data.ucn %]').position();
                    scroll(0,position.top);
				}); 
                
                $('#docketdiv_[% data.ucn %]').on("click", ".showmulti", function () {
                    if ($('#docketform_[% data.ucn %]').find("input[type=checkbox]:checked").length == 0) {
                        $('#dialogSpan').html("Please select one or more items to view.");
                        $('#dialogDiv').dialog({
                            resizable: false,
                            minheight: 150,
                            width: 500,
                            modal: true,
                            title: 'No Items Selected',
                            buttons: {
                            	"OK": function() {
                            		$(this).dialog( "close" );
                            		return false;
                            	}
                            }
                        });
                        return false;
                    }
					
					// Set the pdforder cookie
                    var pdfradio = $("input[name='pdforder']:checked");
                    var pdforder = $(pdfradio).val();
                    $.cookie("pdforder", $(pdfradio).val(), { expires: new Date(2999, 12, 31, 23, 59, 59)});
                    
                    return true;
                });
                
                $(document).on('click','.esButton', function() {
                    var pane = $(this).closest('.caseTab');
                    var casenum = $(pane).find('.ucn').val();
                    
                    var tabName = "case-" + casenum;
                    
                    createTab(tabName,casenum,'casetop','caseTab',1);
                    var esTabName = tabName + '-eservice';
                    createTab(esTabName, 'e-Service', tabName, 'innerPane', 0);
                    
                    if($(this).attr('data-showonly') == "1"){
                    	var postData = {case: casenum, tab: esTabName, show: 1, showOnly: 1};
                    }
                    else{
                    	if($(this).attr('data-efile') == "1"){
                    		var postData = {case: casenum, tab: esTabName, show: 1, efileCheck: 1};
                    	}
                    	else{
                    		var postData = {case: casenum, tab: esTabName, show: 1};
                    	}
                    }
                    
                    $.blockUI({message: '<h1><img src="/images/busy.gif"/> Please Wait </h1>', fadeIn: 0});
                    
                    $.ajax({
                        url: "/cgi-bin/eservice/eService.cgi",
                        data: postData,
                        async: false,
                        success: function(data) {
                            $.unblockUI();
                            showTab(data);
                        }
                    });
                });
                
                var docViewer = CreateDocumentViewer("#documents_[% data.ucn %]", "[% data.ucn %]");
                
                var pane = $('#fullOtherCaseDiv_[% data.ucn %]').closest('.caseTab');
                OpenDefaultView($(pane));
                return true;
            });
			
            </script>
            
