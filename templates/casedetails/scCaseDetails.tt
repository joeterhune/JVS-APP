                                                                    <th style="min-width: 10em">Current Queue</th>
                                                                    <th style="min-width: 6em">Current Status</th>
                                                                    <th style="min-width: 20em">Comments</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                [% FOREACH doc IN data.otherDocs %]
                                                                <tr style="[% doc.style %]">
                                                                    <td class="center">
                                                                    	[% IF doc.file %]
			                                                        		<a href="[% doc.file %]" target="_blank">[% doc.Title %]</a>
			                                                        	[% ELSE %]
			                                                        		<a class="[% doc.doc_type %]" data-is_order="[% doc.is_order %]" data-docid="[% doc.DocID %]" data-casenum="[% data.caseinfo.CaseNumber %]">[% doc.Title %]</a></td>
			                                                        	[% END %]		
                                                                    </td>
                                                                    <td class="center">[% doc.creator %]</td>
                                                                    <td class="center">[% doc.CreationDate %]</td>
                                                                    <td class="center">[% doc.queue %]</td>
                                                                    <td class="center">[% doc.current_status %]</td>
                                                                    <td class="center">
                                                                        [% doc.Comments %]
                                                                    </td>
                                                                </tr>
                                                                [% END %]
                                                                <tr>
			                                                    	<td colspan="6" align="center">
			                                                    		<a class="showAllDocs">Show Hidden Documents</a>
			                                                    		<a class="hideDocs" style="display:none">Hide Completed Documents</a>
			                                                    	</td>
			                                                    </tr>
                                                            </tbody>
                                                        </table>
                                                    </td>
                                                </tr>
                                                [% END %]
                                            </table>
                                        </div>
                                    </td>
                                </tr>
                                
            
                                <!-- Docket -->
                                <tr class="spacer"><td>&nbsp;</td></tr>
                                <tr>
                                    <td>
                                        <div class="docketdiv" id="docketdiv_[% data.ucn %]">
                                            Loading Docket Info...
                                        </div>
                                    </td>
                                </tr>
            
                                <!-- Other Cases -->
                                <tr class="spacer"><td>&nbsp;</td></tr>
                                <tr>
                                    <td>
                                        <div style="position: relative" id="fullOtherCaseDiv_[% data.ucn %]">
                                            Loading Other Case Listing...
                                        </div>
                                    </td>
                                </tr>
            
                                <!-- Reopens -->
                                <tr class="spacer"><td>&nbsp;</td></tr>
                                <tr>
                                    <td>
                                        <div id="reopendiv_[% data.ucn %]" title="Case Reopen History">
                                            <table class="summary">
                                                <tr>
                                                    <td class="title">Reopen History</td>
                                                </tr>
                                                <tr>
                                                    [% IF data.reopens.size == 0 %]
                                                    <td>There is no reopen history for this case.</td>
                                                    [% ELSE %]
                                                    <td class="tableholder">
                                                        <table id="reopens_[% data.ucn %]">
                                                            <thead>
                                                                <tr class="title">
                                                                    <th style="min-width: 7em">Count</th>
                                                                    <th style="min-width: 10em">Reason</th>
                                                                    <th style="min-width: 6em">Reopen<br/>Date</th>
                                                                    <th style="min-width: 6em">Reopen<br/>Close Date</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                            [% FOREACH reopen IN data.reopens %]
                                                                <tr class="[% event.RowClass %]">
                                                                    <td style="text-align: center">[% reopen.CountNo %]</td>
                                                                    <td style="text-align: center">[% reopen.ReOpenReason %]</td>
                                                                    <td style="text-align: center">[% reopen.ReOpenDate %]</td>
                                                                    <td style="text-align: center">[% reopen.ReOpenCloseDate %]</td>
                                                                </tr>
                                                            [% END %]
                                                            </tbody>
                                                        </table>
                                                    </td>
                                                    [% END %]
                                                </tr>
                                            </table>
                                        </div>
                                    </td>
                                </tr>
            
            
                            </table>
                        </div>
            
                        <div style="padding-top:1%">
                            <div class="pbsodiv" id="pbsodiv_[% data.ucn %]">
                                Retrieving PBSO Booking History...
                            </div>
                        </div>
                    
                        <div id="hiddenvars_[% data.ucn %]">
                            <input class="division" type="hidden" id="division_[% data.ucn %]" name="division" value="[% data.caseinfo.DivisionID %]"/>
                            <input class="mjid" type="hidden" id="mjid_[% data.ucn %]" name="mjid" value="[% data.MJID %]"/>
                            <input class="ucn" type="hidden" id="ucn_[% data.ucn %]" name="ucn" value="[% data.caseinfo.CaseNumber %]"/>
                            <input class="caseType" type="hidden" value="[% data.caseinfo.CaseType %]"/>
                            <input class="caseid" type="hidden" value="[% data.caseinfo.CaseID %]"/>
                        </div>
                        
                    </div> <!-- end of drawer-content -->
                    
                </div>
                    
                <div class="content closed" style="display: none; height: 100%">
                    <div id="documents_[% data.ucn %]" class="dv-window" style="height: 90%; width: 100%;">
                    </div>
                </div>
            
            </div> <!-- End of drawer-container -->
