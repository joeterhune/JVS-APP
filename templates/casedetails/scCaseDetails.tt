<div style="padding-left:1%">
            <script type="text/javascript">
            $(document).ready(function() {
                        $('#fullOtherCaseDiv_[% data.ucn %]').on("click", ".allCaseDocket", function () {
                        var cases = new Array;
                    var case_ids = new Array;
                    $('#othercases_[% data.ucn %]').find('.otherCaseCheck:checked').each(function (i,e) {
                    	var value = $(e).val().split(';');
                        cases.push(value[0]);
						case_ids.push(value[1]);
                    });
					
                    if (cases.length == 0) {
                        $('#dialogSpan').html("Please select one or more cases to view.");
                        $('#dialogDiv').dialog({
                            resizable: false,
                            minheight: 150,
                            width: 500,
                            modal: true,
                            title: 'No Cases Selected',
                            buttons: {
                                "OK": function() {
                                    $(this).dialog( "close" );
                                    return false;
                                }
                            }
                        });
                        
                        return false;
                    }
                    
                    var casenum = "[% data.ucn %]";
                    var query = cases.join();
					var query2 = case_ids.join();
                    var url = "/cgi-bin/scAllCaseDockets.cgi";
                    window.location.href = url + "?casenums=" + query + "&caseids=" + query2 + "&casenum=" + casenum;
                    return false;
				});
				
                scGetAsync('[% data.ucn %]', '[% data.caseinfo.CaseNumber %]', '[% data.MJID %]', '[% data.caseinfo.CaseType %]', '[% data.caseinfo.CaseID %]');
				
                $("table thead th:eq(6)").data("sorter", false);
                $('#attorneys_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#charges_[% data.ucn %]').tablesorter({widgets: ['zebra'], headers: {0: {sorter: false}}});
                $('#sentences_[% data.ucn %]').tablesorter({widgets: ['zebra'], headers: {0: {sorter: false}}});
                $('#events_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#bonds_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#related_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#arrests_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#warrants_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#reopens_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                $('#citations_[% data.ucn %]').tablesorter({widgets: ['zebra']});
                
                $('#docketdiv_[% data.ucn %]').on("click", ".showmulti", function () {
                    if ($('#docketform_[% data.ucn %]').find("input[type=checkbox]:checked").length == 0) {
                        $('#dialogSpan').html("Please select one or more items to view.");
                        $('#dialogDiv').dialog({
                            resizable: false,
                            minheight: 150,
                            width: 500,
                            modal: true,
                            title: 'No Items Selected',
                            buttons: {
                            	"OK": function() {
                            		$(this).dialog( "close" );
                            		return false;
                            	}
                            }
                        });
                        return false;
                    }
                    // Set the pdforder cookie
                    var pdfradio = $("input[name='pdforder']:checked");
                    var pdforder = $(pdfradio).val();
                    $.cookie("pdforder", $(pdfradio).val(), { expires: new Date(2999, 12, 31, 23, 59, 59)});
                    
                    return true;
                });
                
                $('#pbsodiv_[% data.ucn %]').on("click", ".pbsoview",function () {
                    var bookingID = $(this).parent().parent().find('.bookingID').first().val();
                    var url = "/cgi-bin/pbsobview.cgi?jacket=" + $('#mjid_[% data.ucn %]').val() + "&booking=" + bookingID + "&lev=" + $('#nextlev').val() + "&ucn=[% data.ucn %]";
                    window.location.replace(url);
                    return true;
                });
                
                $(document).on('click','.docLink', function(e) {
	                e.preventDefault();
	                // Handle clicking on the document name.
	                var ucn = $(this).data('casenum');
	                var docid = $(this).data('docid');
	                var is_order = $(this).data('is_order');
	                window.location.href = "/orders/preview.php?fromWF=1&ucn=" + ucn + "&docid=" + docid + "&isOrder=" + is_order;
	            });
	            
	            $(document).on("click", ".showAllDocs", function () {
	            	$.each($('#otherdocs_[% data.ucn %] *').not(':visible'), function() {
						$(this).addClass('toHide');
						$(this).show();
					});
					
					$('.showAllDocs').hide();
					$('.hideDocs').show();
					$("#otherdocs_[% data.ucn %]").trigger("update");
				}); 
				
				$(document).on("click", ".hideDocs", function () {
	            	$.each($('.toHide'), function() {
						$(this).removeClass('toHide');
						$(this).hide();
					});
					
					$('.showAllDocs').show();
					$('.hideDocs').hide();
					$("#otherdocs_[% data.ucn %]").trigger("update");
				}); 
				
				if($('#otherdocs_[% data.ucn %] tr:hidden').length < 1){
					$('.showAllDocs').hide();
				}
            });
			
            </script>
            

